{
  "name": "supaspace",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "host": "vite --host",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "createMdFiles": "cp mdsource.md public/appInfo.md && cp mdsource.md README.md && node scripts/replaceVersion.cjs",
    "commitMdFiles": "git add . && git commit -m \"update md files\" || true",
    "gitPushWithMdFiles": "npm run createMdFiles && npm run commitMdFiles && git push"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@supabase/supabase-js": "^2.49.1",
    "@tailwindcss/vite": "^4.0.15",
    "@tanstack/react-query": "^5.69.0",
    "@uiw/react-md-editor": "^4.0.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.56.0",
    "react-photo-view": "^1.2.7",
    "react-router": "^7.4.0",
    "react-toastify": "^11.0.5",
    "tailwindcss": "^4.1.4",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.21.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^15.15.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.24.1",
    "vite": "^6.2.0"
  }
}
